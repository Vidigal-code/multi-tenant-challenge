{
  "name": "multi-tenant-challenge-backend",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "prebuild": "prisma generate",
    "start": "node dist/main.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "ts-node prisma/seed.ts",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "worker:invites": "ts-node -r tsconfig-paths/register src/interfaces/consumers/events/invites.events.consumer.ts",
    "worker:invites-list": "ts-node -r tsconfig-paths/register src/interfaces/consumers/invites/invites.list.consumer.ts",
    "worker:invites-bulk": "ts-node -r tsconfig-paths/register src/interfaces/consumers/invites/invites.bulk.consumer.ts",
    "worker:members": "ts-node -r tsconfig-paths/register src/interfaces/consumers/events/members.events.consumer.ts",
    "worker:companies-list": "ts-node -r tsconfig-paths/register src/interfaces/consumers/companys/company.list.consumer.ts",
    "worker:generic": "ts-node -r tsconfig-paths/register src/interfaces/consumers/events/generic.events.consumer.ts",
    "worker:realtime": "ts-node -r tsconfig-paths/register src/interfaces/consumers/events/realtime-notifications.consumer.ts",
    "worker:notifications-list": "ts-node -r tsconfig-paths/register src/interfaces/consumers/notifications/notification.list.consumer.ts",
    "worker:notifications-delete": "ts-node -r tsconfig-paths/register src/interfaces/consumers/notifications/notification-deletion.consumer.ts",
    "worker:notifications-broadcast": "ts-node -r tsconfig-paths/register src/interfaces/consumers/notifications/notification-broadcast.consumer.ts",
    "worker:notifications-friends-broadcast": "ts-node -r tsconfig-paths/register src/interfaces/consumers/notifications/notification-friend-broadcast.consumer.ts",
    "worker:friendships-list": "ts-node -r tsconfig-paths/register src/interfaces/consumers/friendships/friendship.list.consumer.ts",
    "worker:users-search": "ts-node -r tsconfig-paths/register src/interfaces/consumers/users/user.search.consumer.ts",
    "worker:users-delete": "ts-node -r tsconfig-paths/register src/interfaces/consumers/users/user-deletion.consumer.ts",
    "clean:queues": "powershell -ExecutionPolicy Bypass -File ./scripts/clean-queues.ps1",
    "clear:dedup": "powershell -ExecutionPolicy Bypass -File ./scripts/clear-redis-dedup.ps1"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/client": "^6.19.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.2",
    "nestjs-pino": "^4.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.19.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@types/amqplib": "^0.10.8",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
